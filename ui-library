getgenv().library = {}

function getgenv().library:CreateWindow(title)
    local ScreenGui = Instance.new("ScreenGui")
    local MainFrame = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local MainTitle = Instance.new("TextLabel")
    local TogleOnOff = Instance.new("ImageButton")
    local MainContainer = Instance.new("Frame")
    local UICorner_2 = Instance.new("UICorner")
    local UIGridLayout = Instance.new("UIGridLayout")
    
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ScreenGui
    MainFrame.Active = true
    MainFrame.Draggable = true
    MainFrame.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
    MainFrame.Position = UDim2.new(0.0335070752, 0, 0.0496969707, 0)
    MainFrame.Size = UDim2.new(0, 248, 0, 45)
    
    UICorner.Parent = MainFrame
    
    MainTitle.Name = "MainTitle"
    MainTitle.Parent = MainFrame
    MainTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    MainTitle.BackgroundTransparency = 1.000
    MainTitle.Position = UDim2.new(0.0362903215, 0, 0, 0)
    MainTitle.Size = UDim2.new(0, 179, 0, 45)
    MainTitle.Font = Enum.Font.SourceSansLight
    MainTitle.Text = title
    MainTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    MainTitle.TextSize = 32.000
    MainTitle.TextWrapped = true
    MainTitle.TextXAlignment = Enum.TextXAlignment.Left
    
    TogleOnOff.Name = "TogleOnOff"
    TogleOnOff.Parent = MainFrame
    TogleOnOff.BackgroundColor3 = Color3.fromRGB(253, 251, 255)
    TogleOnOff.BackgroundTransparency = 1.000
    TogleOnOff.Position = UDim2.new(0.875, 0, 0.211111099, 0)
    TogleOnOff.Size = UDim2.new(0, 25, 0, 25)
    TogleOnOff.ZIndex = 2
    TogleOnOff.Image = "rbxassetid://3926307971"
    TogleOnOff.ImageRectOffset = Vector2.new(204, 484)
    TogleOnOff.ImageRectSize = Vector2.new(36, 36)
    
    MainContainer.Name = "MainContainer"
    MainContainer.Parent = MainFrame
    MainContainer.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
    MainContainer.ClipsDescendants = true
    MainContainer.Position = UDim2.new(0, 0, 0.766666651, 0)
    MainContainer.Size = UDim2.new(0, 248, 0, 308)
    MainContainer.ZIndex = -2
    
    UICorner_2.Parent = MainContainer
    
    UIGridLayout.Parent = MainContainer
    UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIGridLayout.CellSize = UDim2.new(1, 0, 0, 30)
    
    local function BXVXZV_fake_script() -- TogleOnOff.ScriptTogle 
        local script = Instance.new('LocalScript', TogleOnOff)
    
        local debounce = true
        local Container = script.Parent.Parent.MainContainer
        local ClosedSize = UDim2.new(0, 248,0, 11)
        local OpenedSize = UDim2.new(0, 248,0, 309)
        local TweenService = game:GetService("TweenService")
        local TweengInfo = TweenInfo.new(0.3, Enum.EasingStyle.Cubic, Enum.EasingDirection.InOut)
        local TogleOnOff = script.Parent
        script.Parent.MouseButton1Click:Connect(function()
            debounce = not debounce
            if debounce == false then
                TweenService:Create(TogleOnOff, TweengInfo, {Rotation = 180}):Play()
                Container:TweenSize(OpenedSize, "Out", "Quart", 0.4, true)
                for i,v in pairs(script.Parent.Parent.MainContainer:GetDescendants()) do
                    if v:IsA("TextButton") or v:IsA("TextLabel") then
                        wait()
                        v.Visible = true
                    end
                end
            else
                TweenService:Create(TogleOnOff, TweengInfo, {Rotation = 0}):Play()
                Container:TweenSize(ClosedSize, "Out", "Quart", 0.4, true)
                wait(0.2)
                for i,v in pairs(script.Parent.Parent.MainContainer:GetDescendants()) do
                    if v:IsA("TextButton") or v:IsA("TextLabel") then
                        v.Visible = false
                    end
                end
            end
        end)
    end
    coroutine.wrap(BXVXZV_fake_script)()
    local function PZYWK_fake_script() -- MainContainer.LocalScript 
        local script = Instance.new('LocalScript', MainContainer)
    
        script.Parent.Size = UDim2.new(0, 248,0, 5)
    end
    coroutine.wrap(PZYWK_fake_script)()

    getgenv().Buttons = {}

    function getgenv().Buttons:CreateButton(title, callback)
        local callback = callback or function()end
        local Button = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")
        
        
        Button.Name = "Button"
        Button.Parent = MainContainer
        Button.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
        Button.Size = UDim2.new(0, 200, 0, 50)
        Button.Font = Enum.Font.SciFi
        Button.Text = title
        Button.TextColor3 = Color3.fromRGB(25, 250, 0)
        Button.TextSize = 26.000
        Button.MouseButton1Click:Connect(function() pcall(callback) end)
        UICorner.CornerRadius = UDim.new(0, 10)
        UICorner.Parent = Button
    end
    function getgenv().Buttons:CreateTogle(title, callback)
        
        local callback = callback or function()end
        local Togle = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")
        local OndeClica = Instance.new("TextLabel")
        local UICorner_2 = Instance.new("UICorner")
        local OndeClica2 = Instance.new("TextLabel")
        local UICorner_3 = Instance.new("UICorner")

        Togle.Name = "Togle"
        Togle.Parent = MainContainer
        Togle.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
        Togle.Size = UDim2.new(0, 200, 0, 50)
        Togle.Font = Enum.Font.SciFi
        Togle.Text = title
        Togle.TextColor3 = Color3.fromRGB(25, 250, 0)
        Togle.TextSize = 26.000
        pcall(function()
            local Enabled = false
            function Turn()
                Enabled = not Enabled
            end
            Togle.MouseButton1Click:Connect(function()
                Turn()
                pcall(callback, Enabled)
            end)
        end)
        

        UICorner.CornerRadius = UDim.new(0, 10)
        UICorner.Parent = Togle

        OndeClica.Name = "OndeClica"
        OndeClica.Parent = Togle
        OndeClica.AnchorPoint = Vector2.new(0.5, 0.5)
        OndeClica.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        OndeClica.Position = UDim2.new(0.927419364, 0, 0.5, 0)
        OndeClica.Size = UDim2.new(0, 23, 0, 23)
        OndeClica.Font = Enum.Font.SourceSans
        OndeClica.Text = ""
        OndeClica.TextColor3 = Color3.fromRGB(0, 0, 0)
        OndeClica.TextSize = 14.000

        UICorner_2.Parent = OndeClica

        OndeClica2.Name = "OndeClica2"
        OndeClica2.Parent = OndeClica
        OndeClica2.AnchorPoint = Vector2.new(0.5, 0.5)
        OndeClica2.BackgroundColor3 = Color3.fromRGB(17, 255, 0)
        OndeClica2.Position = UDim2.new(0.492217392, 0, 0.5, 0)
        OndeClica2.Font = Enum.Font.SourceSans
        OndeClica2.Text = ""
        OndeClica2.TextColor3 = Color3.fromRGB(0, 0, 0)
        OndeClica2.TextSize = 14.000

        UICorner_3.Parent = OndeClica2
        local function NVIQ_fake_script() -- Togle.LocalScript 
            local script = Instance.new('LocalScript', Togle)

            local a = true
            
            script.Parent.MouseButton1Click:Connect(function()
                a = not a
                if a == false then
                    script.Parent.OndeClica.OndeClica2:TweenSize(UDim2.new(0, 23,0, 23), "Out", "Quart", 0.2, true)
                else
                    script.Parent.OndeClica.OndeClica2:TweenSize(UDim2.new(0, 0,0, 0), "Out", "Quart", 0.2, true)
                end
            end)
        end
        coroutine.wrap(NVIQ_fake_script)()
    end

    return getgenv().Buttons
end

