local message = Instance.new("Message", game.CoreGui)
message.Text = "Bring gui is loaded - Made by ilxsxmu"
game.Workspace.CurrentCamera.CFrame = CFrame.new()
delay(2, function()
    message:Destroy()
end)
function SendNotification(title, text, duration)
    game.StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = duration;
        })
end
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.0953090116, 0, 0.301818192, 0)
Frame.Size = UDim2.new(0, 182, 0, 100)
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 170, 23)
TextLabel.Size = UDim2.new(0, 182, 0, 26)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "Bring gui - made by ilxsxmu"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextWrapped = true

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(15, 51, 255)
TextBox.Position = UDim2.new(0, 0, 0.25999999, 0)
TextBox.Size = UDim2.new(0, 182, 0, 42)
TextBox.Font = Enum.Font.SourceSans
TextBox.Text = "Player name"
TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
TextBox.TextSize = 23.000
TextBox.TextWrapped = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(41, 255, 8)
TextButton.Position = UDim2.new(0, 0, 0.680000007, 0)
TextButton.Size = UDim2.new(0, 182, 0, 32)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "Bring"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 27.000
TextButton.MouseButton1Down:Connect(function()
    SendNotification("Bring gui", "Find player...", 1)
    if game.Players:FindFirstChild(TextBox.text) then
        SendNotification("Bring gui", "Please equip tool", 1)
        local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local NowPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        local Player = game.Players:FindFirstChild(TextBox.Text)
        local Character = Player.Character or Player.CharacterAdded:wait()
        SendNotification("Bring gui", "Player found", 1)
        local Now = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        game.Players.LocalPlayer.Character.Humanoid.Name = 1
        local l = game.Players.LocalPlayer.Character["1"]:Clone()
        game.Players.LocalPlayer.Character.Animate.Disabled = true
        if game.Players.LocalPlayer.Character["1"] then
            l.Parent = game.Players.LocalPlayer.Character
            
            game.Workspace.CurrentCamera.CameraSubject = Character
            l.Name = "Humanoid"
            wait()
            game.Players.LocalPlayer.Character["1"]:Destroy()
            wait()
            delay(0.8, function()
                for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v:IsA("Tool") then
                        function Equip()
                            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                        end
                        if v:FindFirstChild("Handle") then
                            Equip()
                        end
                    end
                end
            end)
            for i = 1,50 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
                wait()
            end
            for i = 1,20 do
                game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
                wait()
            end
            local function callback(text)
                if text == "Yes" then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
                end
            end

            local BindableFunction = Instance.new("BindableFunction")
            BindableFunction.OnInvoke = callback

            game.StarterGui:SetCore("SendNotification", {
                Title = "Bring gui";
                Text = "Teleport to old position?";
                Button1 = "Yes";
                Button2 = "No";
                Duration = 999999;
                Callback = BindableFunction;
            })
            wait(1.8)
            repeat
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = Now
                wait()
            until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - NowPos).Magnitude <=5


        end
    end
end)
wait(3)
pcall(function()
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    local TextBox2 = Instance.new("TextBox")
    local TextButton = Instance.new("TextButton")
    
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.0953090116, 0, 0.301818192, 0)
    Frame.Size = UDim2.new(0, 182, 0, 100)
    Frame.Active = true
    Frame.Draggable = true
    
    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 170, 23)
    TextLabel.Size = UDim2.new(0, 182, 0, 26)
    TextLabel.Font = Enum.Font.SourceSans
    TextLabel.Text = "Teleport gui - made by ilxsxmu"
    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 14.000
    TextLabel.TextWrapped = true
    
    TextBox2.Parent = Frame
    TextBox2.BackgroundColor3 = Color3.fromRGB(15, 51, 255)
    TextBox2.Position = UDim2.new(0, 0, 0.25999999, 0)
    TextBox2.Size = UDim2.new(0, 182, 0, 42)
    TextBox2.Font = Enum.Font.SourceSans
    TextBox2.Text = "Player name"
    TextBox2.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextBox2.TextSize = 23.000
    TextBox2.TextWrapped = true
    
    TextButton.Parent = Frame
    TextButton.BackgroundColor3 = Color3.fromRGB(41, 255, 8)
    TextButton.Position = UDim2.new(0, 0, 0.680000007, 0)
    TextButton.Size = UDim2.new(0, 182, 0, 32)
    TextButton.Font = Enum.Font.SourceSans
    TextButton.Text = "Teleport"
    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton.TextSize = 27.000
    TextButton.MouseButton1Down:Connect(function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[TextBox2.Text].Character.HumanoidRootPart.CFrame
    end)
    local function callback(text)
        if text == "Yes" then
            spawn(function()
                local message = Instance.new("Message", game.CoreGui)
                message.Text = "Fast nick gui is loaded - Made by ilxsxmu"
                game.Workspace.CurrentCamera.CFrame = CFrame.new()
                delay(2, function()
                    message:Destroy()
                end)
                wait()
                local ScreenGui = Instance.new("ScreenGui")
                local Frame_Um = Instance.new("Frame")
                local Titulo = Instance.new("TextLabel")
                local Frame_Dois = Instance.new("Frame")
                local UIGridLayout = Instance.new("UIGridLayout")
                local BotaoPlayer = Instance.new("TextButton")
                
                
                ScreenGui.Parent = game.CoreGui
                ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                
                Frame_Um.Name = "Frame_Um"
                Frame_Um.Parent = ScreenGui
                Frame_Um.BackgroundColor3 = Color3.fromRGB(23, 58, 255)
                Frame_Um.Position = UDim2.new(0.699925542, 0, 0.26303032, 0)
                Frame_Um.Size = UDim2.new(0, 293, 0, 445)
                Frame_Um.Active = true
                Frame_Um.Draggable = true
                
                Titulo.Name = "Titulo"
                Titulo.Parent = Frame_Um
                Titulo.BackgroundColor3 = Color3.fromRGB(27, 20, 255)
                Titulo.Size = UDim2.new(0, 293, 0, 53)
                Titulo.Font = Enum.Font.SourceSans
                Titulo.Text = "fast nicks - Made by ilxsxmu"
                Titulo.TextColor3 = Color3.fromRGB(0, 0, 0)
                Titulo.TextSize = 29.000
                Titulo.TextWrapped = true
                
                Frame_Dois.Name = "Frame_Dois"
                Frame_Dois.Parent = Frame_Um
                Frame_Dois.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Frame_Dois.BackgroundTransparency = 1.000
                Frame_Dois.Position = UDim2.new(0, 0, 0.141573027, 0)
                Frame_Dois.Size = UDim2.new(0, 293, 0, 382)
                
                UIGridLayout.Parent = Frame_Dois
                UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIGridLayout.CellSize = UDim2.new(0, 290, 0, 30)
                
                BotaoPlayer.Name = "BotaoPlayer"
                BotaoPlayer.Parent = Frame_Um
                BotaoPlayer.BackgroundColor3 = Color3.fromRGB(0, 4, 255)
                BotaoPlayer.Size = UDim2.new(0, 200, 0, 50)
                BotaoPlayer.Visible = false
                BotaoPlayer.Font = Enum.Font.SourceSans
                BotaoPlayer.TextColor3 = Color3.fromRGB(0, 0, 0)
                BotaoPlayer.TextSize = 14.000
                
                while true do
                for i,v in pairs(game.Players:GetChildren()) do
                    if v then
                        local w = BotaoPlayer:Clone()
                        w.Parent = Frame_Dois
                        w.Text = v.Name
                        w.TextScaled = true
                        w.Visible = true
                        delay(0.3, function()
                            w:Destroy()
                        end)
                        w.MouseButton1Down:Connect(function()
                            TextBox.Text = v.Name
                            TextBox2.Text = v.Name
                        end)
                    end
                end
                wait(0.3)
                end
            end)
        end
    end

    local BindableFunction = Instance.new("BindableFunction")
    BindableFunction.OnInvoke = callback

    game.StarterGui:SetCore("SendNotification", {
        Title = "Bring gui";
        Text = "Need gui Fast nick?";
        Button1 = "Yes";
        Button2 = "No";
        Callback = BindableFunction;
    })
end)

